class DemoAbstraction
{
	public static void main (String [] args)
	{
	
	A a=new B();
	  a.sum(12,13);
	  a.print();
	  a.print(10,17);
	}
}
abstract class A
{
	abstract void print();
	abstract void print(int p, int q);
	public void sum(int x, int y)
	{
		int add=x+y;
		System.out.println(add);
	}
}
class B extends A
{
	public void print()
	{
		System.out.println("Abstraction done.... ");
	}
	public void print(int k, int l)
	{
		System.out.println("Abstraction done.... ");
	}
}
