public class Ex1 {
    public static void main(String []args)
    {
        try{
            int n1=30, n2=0;
            int output=n1/n2;
            System.out.println(output);
        }
        catch(ArithmeticException e){
            System.out.println("you shoudn't divide a number by zero");
        }
    
    }
    
}
