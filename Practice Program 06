//Swapping Two Numbers Using Bitwise


package Activities;

import java.util.Scanner;


public class SwappingTwoNumbersUsingBitwise {
    public static void main (String [] args)
    {
         int num1,num2;
         Scanner str=new Scanner(System.in);
    
         System.out.print("Enter number 1 : ");
          num1= str.nextInt();
         
         System.out.print("Enter number 2 : ");
         num2= str.nextInt();
            
         num1=num1 ^ num2;
         num2=num1 ^ num2;
         num1=num1 ^ num2;
         str.close();
         
         System.out.println("1st number after swiping : " +num1);
         System.out.println("2nd number after swiping : " +num2);
    }
    
}


package Activities;

import java.util.Scanner;


public class SwappingTwoNumbersUsingBitwise {
    public static void main (String [] args)
    {
         int num1,num2;
         Scanner str=new Scanner(System.in);
    
         System.out.print("Enter number 1 : ");
          num1= str.nextInt();
         
         System.out.print("Enter number 2 : ");
         num2= str.nextInt();
            
         num1=num1 ^ num2;
         num2=num1 ^ num2;
         num1=num1 ^ num2;
         str.close();
         
         System.out.println("1st number after swiping : " +num1);
         System.out.println("2nd number after swiping : " +num2);
    }
    
}
