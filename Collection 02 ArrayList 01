
package Collection;
import java.util.*;

public class DemoArrayList {
    public static void main(String []args)
    {
        ArrayList<String> alist=new ArrayList<String>();
        alist.add("Vindi");
        alist.add("Hansini");
        alist.add("Ama");
        alist.add("Thathsarani");
        alist.add("Lee min hoo");
        alist.add("Songi");
        
        System.out.println(alist);
        
        alist.add(2,"Vithanage");
        System.out.println(alist);
        
    }
}
